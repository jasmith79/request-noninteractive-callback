{
  "name": "@jasmith79/request-noninteractive-callback",
  "version": "0.0.1",
  "description": "Allows scheduling a callback to run once a user quits interacting with an element",
  "main": "dist/index.js",
  "scripts": {
    "test": "jest src",
    "type-check": "tsc --noEmit",
    "type-check:watch": "npm run type-check -- --watch",
    "build": "npm run build:ts && npm run gen-docs",
    "build:types": "tsc --emitDeclarationOnly",
    "build:ts": "tsc --outDir ./dist",
    "gen-docs": "typedoc --readme README.md --excludeNotExported --ignoreCompilerErrors --exclude '{**/*spec*,**/node_modules/**,**/spec/**}' --out docs ./src; touch docs/.nojekyll"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jasmith79/request-noninteractive-callback.git"
  },
  "keywords": [
    "DOM",
    "Event",
    "callback"
  ],
  "author": "jasmith79",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jasmith79/request-noninteractive-callback/issues"
  },
  "homepage": "https://github.com/jasmith79/request-noninteractive-callback#readme",
  "devDependencies": {
    "@types/jest": "^25.2.1",
    "jest": "^25.3.0",
    "ts-jest": "^25.3.1",
    "typedoc": "^0.17.4",
    "typescript": "^3.8.3"
  }
}
